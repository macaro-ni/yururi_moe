<div class="container">
  <div class="row justify-content-center">
    <h3>Contact　確認</h3>
  </div>
  <div class="row justify-content-center">
    <ul class="list-unstyled">
        <%= form_with model: @contact, url: contacts_path do |f| %>

        <%= @contact.name %>
        <%= @contact.email %>
        <%= @contact.message %>

          <li class="text-center"><%= f.submit "Submit", class: "btn btn:hover" %></li>

        <% end %>
    </ul>
  </div>
</div>